//===========================================================================
// Updates the values of all preset conditions
//===========================================================================
function UpdateConditions takes nothing returns nothing
    set gCond_BuildGeneral = ( TownThreatened(  ) == false )
    set gCond_BuildTowers = ( ( TownThreatened(  ) == false ) and ( ( ( GetUnitCount( 'negm' ) + GetUnitCount( 'negf' ) ) + GetUnitCount( 'negt' ) ) < 9 ) )
    set gCond_HeroFirst = ( GetUnitCountDone( 'nfrt' ) > 0 )
    set gCond_HeroSecond = ( ( GetUnitCount( 'nfrt' ) > 0 ) and ( ( GetUnitCountDone( 'nef0' ) > 0 ) or ( GetUnitCountDone( 'nef1' ) > 0 ) ) )
    set gCond_HeroThird = ( ( GetUnitCount( 'nfrt' ) > 0 ) and ( GetUnitCountDone( 'nef1' ) > 0 ) )
    set gCond_BuildFarm = ( ( TownThreatened(  ) == false ) and ( ( TotalFoodProduced(  ) < 200 ) and ( ( TotalFoodProduced(  ) - FoodUsed(  ) ) < 15 ) ) )
    set gCond_TrainFootmen = ( ( GetUnitCountDone( 'nef1' ) == 0 ) or ( GetUnitCountDone( 'nef6' ) == 0 ) )
    set gCond_BuildTownHall = ( ( TownThreatened(  ) == false ) and ( ( GetUnitCount( 'ngwr' ) == 0 ) and ( ( GetUnitCount( 'nef0' ) == 0 ) and ( GetUnitCount( 'nef1' ) == 0 ) ) ) )
    set gCond_UpgradeKeep = ( ( TownThreatened(  ) == false ) and ( ( GetUnitCount( 'ngwr' ) > 0 ) and ( ( GetUnitCount( 'nef0' ) == 0 ) and ( GetUnitCount( 'nef1' ) == 0 ) ) ) )
    set gCond_UpgradeCastle = ( ( TownThreatened(  ) == false ) and ( ( GetUnitCount( 'nef0' ) > 0 ) and ( GetUnitCount( 'nef1' ) == 0 ) ) )
    set gCond_NeedExp1 = ( GetMinesOwned(  ) < 2 )
    set gCond_Workers1 = ( ( GetUnitCount( 'nbee' ) < 35 ) and ( ( TownHasMine( ( TownWithMine(  ) + 0 ) ) == true ) and ( TownHasHall( ( TownWithMine(  ) + 0 ) ) == true ) ) )
    set gCond_Workers2 = ( ( GetUnitCount( 'nbee' ) < 35 ) and ( ( TownHasMine( ( TownWithMine(  ) + 1 ) ) == true ) and ( TownHasHall( ( TownWithMine(  ) + 1 ) ) == true ) ) )
    set gCond_Workers3 = ( ( GetUnitCount( 'nbee' ) < 35 ) and ( ( TownHasMine( ( TownWithMine(  ) + 2 ) ) == true ) and ( TownHasHall( ( TownWithMine(  ) + 2 ) ) == true ) ) )
    set gCond_NeedExp2 = ( GetMinesOwned(  ) < 3 )
    set gCond_MinimumGuardGolem = ( ( GetUnitCountDone( 'nef1' ) > 0 ) and ( GetUnitCountDone( 'nef6' ) > 0 ) )
    set gCond_AttackSwordmen = ( GetUnitCount( 'hhes' ) > 0 )
    set gCond_AttackArchers = ( GetUnitCount( 'nhea' ) > 0 )
    set gCond_AttackSiegeGolems = ( GetUnitCount( 'hhdl' ) > 0 )
    set gCond_AttackSorceress = ( GetUnitCount( 'nchp' ) > 0 )
    set gCond_AttackPriests = ( GetUnitCount( 'nmed' ) > 0 )
    set gCond_AttackSpellbreakers = ( GetUnitCount( 'nhym' ) > 0 )
    set gCond_AttackBallistas = ( GetUnitCount( 'hrdh' ) > 0 )
    set gCond_AttackGlaiveThrowers = ( GetUnitCount( 'hbew' ) > 0 )
    set gCond_AttackDragonHawkRiders = ( GetUnitCount( 'hdhw' ) > 0 )
    set gCond_AttackGryphonArchers = ( GetUnitCount( 'njks' ) > 0 )
    set gCond_AttackHippogryphonLancers = ( GetUnitCount( 'nbel' ) > 0 )
    set gCond_TrainArchers = ( ( GetUnitCountDone( 'nef0' ) == 0 ) or ( GetUnitCountDone( 'nef7' ) == 0 ) )
    set gCond_AtacarAEnemigo = ( FoodUsed(  ) > 100 )
    set gCond_Farmeo_avanzado = ( FoodUsed(  ) > 87 )
    set gCond_Farmeo_Mediano = ( FoodUsed(  ) > 57 )
