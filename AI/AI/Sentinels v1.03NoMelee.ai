//===========================================================================
// Updates the values of all preset conditions
//===========================================================================
function UpdateConditions takes nothing returns nothing
    set gCond_BuildGeneral = ( TownThreatened(  ) == false )
    set gCond_BuildTowers = ( ( TownThreatened(  ) == false ) and ( GetUnitCount( 'etrp' ) < 10 ) )
    set gCond_BuildMoonWell = ( ( TownThreatened(  ) == false ) and ( ( ( GetUnitCount( 'emow' ) * 20 ) < 200 ) and ( ( ( GetUnitCount( 'emow' ) * 20 ) - FoodUsed(  ) ) < 20 ) ) )
    set gCond_BuildTreeOfLife = ( ( TownThreatened(  ) == false ) and ( ( GetUnitCount( 'etol' ) == 0 ) and ( ( GetUnitCount( 'etoa' ) == 0 ) and ( GetUnitCount( 'etoe' ) == 0 ) ) ) )
    set gCond_BuildTreeOfAges = ( ( TownThreatened(  ) == false ) and ( ( GetUnitCountDone( 'etol' ) > 0 ) and ( ( GetUnitCount( 'etoa' ) == 0 ) and ( GetUnitCount( 'etoe' ) == 0 ) ) ) )
    set gCond_BuildTreeOfEternity = ( ( TownThreatened(  ) == false ) and ( ( GetUnitCountDone( 'etoa' ) > 0 ) and ( GetUnitCount( 'etoe' ) == 0 ) ) )
    set gCond_HeroFirst = ( GetUnitCountDone( 'eate' ) > 0 )
    set gCond_HeroSecond = ( ( GetUnitCountDone( 'eate' ) > 0 ) and ( ( GetUnitCountDone( 'etoa' ) > 0 ) or ( GetUnitCountDone( 'etoe' ) > 0 ) ) )
    set gCond_HeroThird = ( ( GetUnitCountDone( 'eate' ) > 0 ) and ( GetUnitCountDone( 'etoe' ) > 0 ) )
    set gCond_Workers1 = ( ( GetUnitCount( 'ewsp' ) < 35 ) and ( ( TownHasMine( ( TownWithMine(  ) + 0 ) ) == true ) and ( TownHasHall( ( TownWithMine(  ) + 0 ) ) == true ) ) )
    set gCond_Workers2 = ( ( GetUnitCount( 'ewsp' ) < 35 ) and ( ( TownHasMine( ( TownWithMine(  ) + 1 ) ) == true ) and ( TownHasHall( ( TownWithMine(  ) + 1 ) ) == true ) ) )
    set gCond_Workers3 = ( ( GetUnitCount( 'ewsp' ) < 35 ) and ( ( TownHasMine( ( TownWithMine(  ) + 2 ) ) == true ) and ( TownHasHall( ( TownWithMine(  ) + 2 ) ) == true ) ) )
    set gCond_NeedExp = ( GetMinesOwned(  ) < 2 )
    set gCond_NeedExp2 = ( GetMinesOwned(  ) < 3 )
    set gCond_AttackWithArchers = ( GetUnitCountDone( 'earc' ) > 0 )
    set gCond_AttackWithHuntress = ( GetUnitCountDone( 'esen' ) > 0 )
    set gCond_AttackWithGlaiveThrowers = ( GetUnitCountDone( 'ebal' ) > 0 )
    set gCond_AttackWithDryades = ( GetUnitCountDone( 'edry' ) > 0 )
    set gCond_AttackWithDruidOfClaw = ( GetUnitCountDone( 'edoc' ) > 0 )
    set gCond_AttackWithMountainGiant = ( GetUnitCountDone( 'emtg' ) > 0 )
    set gCond_AttackWithHippogryphs = ( GetUnitCountDone( 'ehip' ) > 0 )
    set gCond_AttackWithDruidOfTalon = ( GetUnitCountDone( 'edot' ) > 0 )
    set gCond_AttackWithFaerieDragons = ( GetUnitCountDone( 'efdr' ) > 0 )
    set gCond_AttackWithChimera = ( GetUnitCountDone( 'echm' ) > 0 )
    set gCond_AtacarAEnemigo = ( FoodUsed(  ) > 100 )
    set gCond_Farmeo_avanzado = ( FoodUsed(  ) > 87 )
    set gCond_Farmeo_Mediano = ( FoodUsed(  ) > 57 )
    set gCond_NeedExpansionRequiere = ( ( FoodUsed(  ) > 42 ) and ( ExpansionNeeded(  ) == true ) )
    set gCond_RushCommand = ( CheckLastCommand( false ) == 1 )
    set gCond_RushForces = ( FoodUsed(  ) < 75 )
endfunction

