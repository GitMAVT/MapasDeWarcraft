//===========================================================================
// Updates the values of all preset conditions
//===========================================================================
function UpdateConditions takes nothing returns nothing
    set gCond_BuildGeneral = ( TownThreatened(  ) == false )
    set gCond_BuildFarms = ( ( TownThreatened(  ) == false ) and ( ( ( GetUnitCount( 'nnfm' ) * 25 ) < 200 ) and ( ( ( GetUnitCount( 'nnfm' ) * 25 ) - FoodUsed(  ) ) < 25 ) ) )
    set gCond_BuildTowers = ( ( TownThreatened(  ) == false ) and ( ( GetUnitCount( 'nntg' ) + GetUnitCount( 'n017' ) ) < 10 ) )
    set gCond_BuildTownHall = ( ( TownThreatened(  ) == false ) and ( ( GetUnitCount( 'nntt' ) == 0 ) and ( ( GetUnitCount( 'n016' ) == 0 ) and ( GetUnitCount( 'n015' ) == 0 ) ) ) )
    set gCond_BuildKeep = ( ( TownThreatened(  ) == false ) and ( ( GetUnitCountDone( 'nntt' ) > 0 ) and ( ( GetUnitCount( 'n016' ) == 0 ) and ( GetUnitCount( 'n015' ) == 0 ) ) ) )
    set gCond_BuildCastle = ( ( TownThreatened(  ) == false ) and ( ( GetUnitCountDone( 'n016' ) > 0 ) and ( GetUnitCount( 'n015' ) == 0 ) ) )
    set gCond_NeedExp = ( GetMinesOwned(  ) < 2 )
    set gCond_NeedExp2 = ( GetMinesOwned(  ) < 3 )
    set gCond_Workers1 = ( ( GetUnitCount( 'nmpe' ) < 35 ) and ( ( TownHasMine( ( TownWithMine(  ) + 0 ) ) == true ) and ( TownHasHall( ( TownWithMine(  ) + 0 ) ) == true ) ) )
    set gCond_Workers2 = ( ( GetUnitCount( 'nmpe' ) < 35 ) and ( ( TownHasMine( ( TownWithMine(  ) + 1 ) ) == true ) and ( TownHasHall( ( TownWithMine(  ) + 1 ) ) == true ) ) )
    set gCond_Workers3 = ( ( GetUnitCount( 'nmpe' ) < 35 ) and ( ( TownHasMine( ( TownWithMine(  ) + 2 ) ) == true ) and ( TownHasHall( ( TownWithMine(  ) + 2 ) ) == true ) ) )
    set gCond_HeroFirst = ( GetUnitCountDone( 'nnad' ) > 0 )
    set gCond_HeroSecond = ( ( GetUnitCountDone( 'nnad' ) > 0 ) and ( ( GetUnitCountDone( 'n016' ) > 0 ) or ( GetUnitCountDone( 'n015' ) > 0 ) ) )
    set gCond_HeroThird = ( ( GetUnitCountDone( 'nnad' ) > 0 ) and ( GetUnitCountDone( 'n015' ) > 0 ) )
    set gCond_AttackWithArchers = ( GetUnitCountDone( 'nnmg' ) > 0 )
    set gCond_AttackWithSnapDragons = ( GetUnitCountDone( 'nsnp' ) > 0 )
    set gCond_AttackWithMyrmidons = ( GetUnitCountDone( 'nmyr' ) > 0 )
    set gCond_AttackWithSeaGiants = ( GetUnitCountDone( 'nnrg' ) > 0 )
    set gCond_AttackWithDragonTurtle = ( GetUnitCountDone( 'nhyc' ) > 0 )
    set gCond_AttackWithWaterDragon = ( GetUnitCountDone( 'h02D' ) > 0 )
    set gCond_AttackWithShadowSerpent = ( GetUnitCountDone( 'u015' ) > 0 )
    set gCond_AttackWithSiren = ( GetUnitCountDone( 'nnsw' ) > 0 )
    set gCond_AttackWithSkyWitch = ( GetUnitCountDone( 'n058' ) > 0 )
    set gCond_AttackWithCouatls = ( GetUnitCountDone( 'nwgs' ) > 0 )
    set gCond_AttackWithFaerieSnapDragon = ( GetUnitCountDone( 'e00L' ) > 0 )
    set gCond_AtacarAEnemigo = ( FoodUsed(  ) > 100 )
    set gCond_Farmeo_avanzado = ( FoodUsed(  ) > 87 )
    set gCond_Farmeo_Mediano = ( FoodUsed(  ) > 57 )
    set gCond_NeedExpansionRequiere = ( ( FoodUsed(  ) > 42 ) and ( ExpansionNeeded(  ) == true ) )
    set gCond_RushCommand = ( CheckLastCommand( false ) == 1 )
