            call AddAttackUnit( all, all, 'o003' )
        endif
        if (gCond_AttacksWithZeppelins) then
            set all = GetUnitCountDone( 'n04H' )
            call AddAttackUnit( all, all, 'n04H' )
        endif
        if (gCond_AttacksWithWarlocks) then
            set all = GetUnitCountDone( 'n04J' )
            call AddAttackUnit( all, all, 'n04J' )
        endif
        if (gCond_AttacksWithNecrolyes) then
