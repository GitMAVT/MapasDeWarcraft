//===========================================================================
// Updates the values of all preset conditions
//===========================================================================
function UpdateConditions takes nothing returns nothing
    set gCond_BuildGeneral = ( TownThreatened(  ) == false )
    set gCond_BuildTowers = ( ( TownThreatened(  ) == false ) and ( GetUnitCount( 'o02B' ) < 10 ) )
    set gCond_BuildFarms = ( ( TownThreatened(  ) == false ) and ( ( ( GetUnitCount( 'o02A' ) * 20 ) < 200 ) and ( ( ( GetUnitCount( 'o02A' ) * 20 ) - FoodUsed(  ) ) < 10 ) ) )
    set gCond_BuildGreatHall = ( ( TownThreatened(  ) == false ) and ( ( GetUnitCount( 'o021' ) == 0 ) and ( ( GetUnitCount( 'o022' ) == 0 ) and ( GetUnitCount( 'o023' ) == 0 ) ) ) )
    set gCond_BuildStronghold = ( ( TownThreatened(  ) == false ) and ( ( GetUnitCountDone( 'o021' ) > 0 ) and ( ( GetUnitCount( 'o022' ) == 0 ) and ( GetUnitCount( 'o023' ) == 0 ) ) ) )
    set gCond_BuildFortress = ( ( TownThreatened(  ) == false ) and ( ( GetUnitCountDone( 'o022' ) > 0 ) and ( GetUnitCount( 'o023' ) == 0 ) ) )
    set gCond_NeedExp = ( GetMinesOwned(  ) < 2 )
    set gCond_NeedExp2 = ( GetMinesOwned(  ) < 3 )
    set gCond_HeroFirst = ( GetUnitCountDone( 'o024' ) > 0 )
    set gCond_HeroSecond = ( ( GetUnitCountDone( 'o024' ) > 0 ) and ( ( GetUnitCountDone( 'o022' ) > 0 ) or ( GetUnitCountDone( 'o023' ) > 0 ) ) )
    set gCond_HeroThird = ( ( GetUnitCountDone( 'o024' ) > 0 ) and ( GetUnitCountDone( 'o023' ) > 0 ) )
    set gCond_Workers1 = ( ( GetUnitCount( 'o02E' ) < 35 ) and ( ( TownHasMine( ( TownWithMine(  ) + 0 ) ) == true ) and ( TownHasHall( ( TownWithMine(  ) + 0 ) ) == true ) ) )
    set gCond_Workers2 = ( ( GetUnitCount( 'o02E' ) < 35 ) and ( ( TownHasMine( ( TownWithMine(  ) + 1 ) ) == true ) and ( TownHasHall( ( TownWithMine(  ) + 1 ) ) == true ) ) )
    set gCond_Workers3 = ( ( GetUnitCount( 'o02E' ) < 35 ) and ( ( TownHasMine( ( TownWithMine(  ) + 2 ) ) == true ) and ( TownHasHall( ( TownWithMine(  ) + 2 ) ) == true ) ) )
    set gCond_AttacksWithGrunts = ( GetUnitCountDone( 'o02F' ) > 0 )
    set gCond_AttacksWithAxethrowers = ( GetUnitCountDone( 'n046' ) > 0 )
    set gCond_AttacksWithOgres = ( GetUnitCountDone( 'n047' ) > 0 )
    set gCond_AttacksWithSappers = ( GetUnitCountDone( 'n04G' ) > 0 )
    set gCond_AttacksWithArmoredMachine = ( GetUnitCountDone( 'o003' ) > 0 )
    set gCond_AttacksWithZeppelins = ( GetUnitCountDone( 'n04H' ) > 0 )
    set gCond_AttacksWithWarlocks = ( GetUnitCountDone( 'n04J' ) > 0 )
    set gCond_AttacksWithNecrolyes = ( GetUnitCountDone( 'n04K' ) > 0 )
    set gCond_AttacksWithPriests = ( GetUnitCountDone( 'n04V' ) > 0 )
    set gCond_AttacksWithRaiders = ( GetUnitCountDone( 'o02U' ) > 0 )
    set gCond_AttacksWithClefhoods = ( GetUnitCountDone( 'n04I' ) > 0 )
    set gCond_AttacksWithRedDragons = ( GetUnitCountDone( 'n04T' ) > 0 )
    set gCond_AttacksWithBerserkers = ( GetUnitCountDone( 'n04L' ) > 0 )
    set gCond_AttacksWithMagis = ( GetUnitCountDone( 'n04M' ) > 0 )
    set gCond_TrainTrollsAxethrowers = ( GetUpgradeLevel( 'R03H' ) == 0 )
    set gCond_TrainTrollsBerserkers = ( GetUpgradeLevel( 'R03H' ) == 1 )
    set gCond_TrainOgres = ( GetUpgradeLevel( 'R03K' ) == 0 )
    set gCond_TrainOgresMagi = ( GetUpgradeLevel( 'R03K' ) == 1 )
    set gCond_AtacarAEnemigo = ( FoodUsed(  ) > 100 )
    set gCond_Farmeo_avanzado = ( FoodUsed(  ) > 87 )
    set gCond_Farmeo_Mediano = ( FoodUsed(  ) > 57 )
