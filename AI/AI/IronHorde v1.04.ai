        if (gCond_AttacksWithAxethrowers) then
            set all = GetUnitCountDone( 'n046' )
            call AddAttackUnit( all, all, 'n046' )
        endif
        if (gCond_AttacksWithBerserkers) then
            set all = GetUnitCountDone( 'n04L' )
            call AddAttackUnit( all, all, 'n04L' )
        endif
        if (gCond_AttacksWithOgres) then
            set all = GetUnitCountDone( 'n047' )
            call AddAttackUnit( all, all, 'n047' )
        endif
