//===========================================================================
// Returns true if the minimum forces for an attack exist
//===========================================================================
function HaveMinimumAttackers takes nothing returns boolean
    local integer count

    // Check for attack wave limit
    if (attackWave > 1) then
        return false
    endif

    // Attack Group: Minimum Units
    if (gCond_TrainRogues) then
        set count = GetUnitCountDone( 'h00J' )
        if (count < 5) then
            return false
        endif
    endif
    if (gCond_MinimumBandits) then
        set count = GetUnitCountDone( 'h00K' )
        if (count < 3) then
            return false
        endif
    endif
    return true
endfunction

//===========================================================================
// Assigns units to attack based on the given attack group
//===========================================================================
function PrepareAttackGroup takes integer groupID returns nothing
    local integer all

    // Attack Group #1: All Units
    if (groupID == 1) then
        set all = GetUnitCountDone( hero_id )
        call AddAttackUnit( all, all, hero_id )
        set all = GetUnitCountDone( hero_id2 )
        call AddAttackUnit( all, all, hero_id2 )
        set all = GetUnitCountDone( hero_id3 )
        call AddAttackUnit( all, all, hero_id3 )
        if (gCond_AttackWithRogues) then
            set all = GetUnitCountDone( 'h00J' )
            call AddAttackUnit( all, all, 'h00J' )
        endif
        if (gCond_AttackWithAssaulter) then
            set all = GetUnitCountDone( 'n010' )
            call AddAttackUnit( all, all, 'n010' )
        endif
        if (gCond_AttackWithBanditLords) then
            set all = GetUnitCountDone( 'h00K' )
            call AddAttackUnit( all, all, 'h00K' )
        endif
        if (gCond_AttackWithFallenPriests) then
            set all = GetUnitCountDone( 'h013' )
            call AddAttackUnit( all, all, 'h013' )
        endif
        if (gCond_AttackWithWizards) then
            set all = GetUnitCountDone( 'u00Z' )
            call AddAttackUnit( all, all, 'u00Z' )
        endif
        if (gCond_AttackWithSpellThief) then
            set all = GetUnitCountDone( 'h01A' )
            call AddAttackUnit( all, all, 'h01A' )
        endif
        if (gCond_AttackWithDragonHawks) then
            set all = GetUnitCountDone( 'h01D' )
            call AddAttackUnit( all, all, 'h01D' )
        endif
        if (gCond_AttackWithGryphons) then
            set all = GetUnitCountDone( 'h01C' )
            call AddAttackUnit( all, all, 'h01C' )
        endif
        if (gCond_AttackWithSasquatches) then
            set all = GetUnitCountDone( 'n012' )
            call AddAttackUnit( all, all, 'n012' )
        endif
        if (gCond_AttackWithOgres) then
            set all = GetUnitCountDone( 'n048' )
            call AddAttackUnit( all, all, 'n048' )
        endif

        // Attack Group #2: Minimum Units
    elseif (groupID == 2) then
        set all = GetUnitCountDone( hero_id )
        call AddAttackUnit( all, all, hero_id )
        if (gCond_TrainRogues) then
            set all = GetUnitCountDone( 'h00J' )
            call AddAttackUnit( all, -1,  'h00J' )
        endif
        if (gCond_MinimumBandits) then
            set all = GetUnitCountDone( 'h00K' )
            call AddAttackUnit( all, -1,  'h00K' )
        endif

    endif
endfunction

//===========================================================================
// Prepares an attack group based on the current attack wave
//===========================================================================
function PrepareForces takes nothing returns nothing
    if (attackWave == 1) then
        call PrepareAttackGroup( 1 )
    endif
endfunction

//===========================================================================
// Sleep delays for each attack wave
//===========================================================================
function AttackWaveDelay takes integer inWave returns nothing
    if (inWave < nextDelay) then
        return
    endif

    set nextDelay = inWave + 1
endfunction

//===========================================================================
// Advances attack wave counter
//===========================================================================
function AttackWaveUpdate takes nothing returns nothing
    call AttackWaveDelay( attackWave )
    set attackWave = attackWave + 1
    if (attackWave > 1) then
        set attackWave = 1
        set nextDelay = attackWave + 1
    endif
endfunction

//===========================================================================
// Basic attack functionality
//===========================================================================
function AttackTarget takes unit target, boolean addAlliance returns nothing
    if (target == null) then
        return
    endif
    if (addAlliance) then
        call SetAllianceTarget( target )
    endif
    call FormGroup( 3, true )
    call AttackMoveKillA( target )
    if (not addAlliance) then
        call SetAllianceTarget( null )
    endif
endfunction

