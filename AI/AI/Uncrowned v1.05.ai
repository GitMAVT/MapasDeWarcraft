//===========================================================================
// Updates the values of all preset conditions
//===========================================================================
function UpdateConditions takes nothing returns nothing
    set gCond_BuildGeneral = ( TownThreatened(  ) == false )
    set gCond_BuildTowers = ( ( TownThreatened(  ) == false ) and ( ( GetUnitCount( 'h00H' ) + ( GetUnitCount( 'h011' ) + GetUnitCount( 'h00X' ) ) ) < 9 ) )
    set gCond_HeroFirst = ( GetUnitCountDone( 'h00C' ) > 0 )
    set gCond_HeroSecond = ( ( GetUnitCountDone( 'h00C' ) > 0 ) and ( ( GetUnitCountDone( 'h00A' ) > 0 ) or ( GetUnitCountDone( 'h00B' ) > 0 ) ) )
    set gCond_HeroThird = ( ( GetUnitCountDone( 'h00C' ) > 0 ) and ( GetUnitCountDone( 'h00B' ) > 0 ) )
    set gCond_BuildFarm = ( ( TownThreatened(  ) == false ) and ( ( TotalFoodProduced(  ) < 200 ) and ( ( TotalFoodProduced(  ) - FoodUsed(  ) ) < 20 ) ) )
    set gCond_TrainRogues = ( GetUnitCountDone( 'h00B' ) == 0 )
    set gCond_BuildTownHall = ( ( TownThreatened(  ) == false ) and ( ( GetUnitCount( 'h005' ) == 0 ) and ( ( GetUnitCount( 'h00A' ) == 0 ) and ( GetUnitCount( 'h00B' ) == 0 ) ) ) )
    set gCond_UpgradeKeep = ( ( TownThreatened(  ) == false ) and ( ( GetUnitCount( 'h005' ) > 0 ) and ( ( GetUnitCount( 'h00A' ) == 0 ) and ( GetUnitCount( 'h00B' ) == 0 ) ) ) )
    set gCond_UpgradeCastle = ( ( TownThreatened(  ) == false ) and ( ( GetUnitCount( 'h00A' ) > 0 ) and ( GetUnitCount( 'h00B' ) == 0 ) ) )
    set gCond_Workers1 = ( ( GetUnitCount( 'h00I' ) < 35 ) and ( ( TownHasMine( ( TownWithMine(  ) + 0 ) ) == true ) and ( TownHasHall( ( TownWithMine(  ) + 0 ) ) == true ) ) )
    set gCond_Workers2 = ( ( GetUnitCount( 'h00I' ) < 35 ) and ( ( TownHasMine( ( TownWithMine(  ) + 1 ) ) == true ) and ( TownHasHall( ( TownWithMine(  ) + 1 ) ) == true ) ) )
    set gCond_Workers3 = ( ( GetUnitCount( 'h00I' ) < 35 ) and ( ( TownHasMine( ( TownWithMine(  ) + 2 ) ) == true ) and ( TownHasHall( ( TownWithMine(  ) + 2 ) ) == true ) ) )
    set gCond_NeedExp2 = ( GetMinesOwned(  ) < 2 )
    set gCond_NeedExp3 = ( GetMinesOwned(  ) < 3 )
    set gCond_AttackWithRogues = ( GetUnitCount( 'h00J' ) > 0 )
    set gCond_AttackWithAssaulter = ( GetUnitCount( 'n010' ) > 0 )
    set gCond_AttackWithBanditLords = ( GetUnitCount( 'h00K' ) > 0 )
    set gCond_AttackWithFallenPriests = ( GetUnitCount( 'h013' ) > 0 )
    set gCond_AttackWithWizards = ( GetUnitCount( 'u00Z' ) > 0 )
    set gCond_AttackWithSpellThief = ( GetUnitCount( 'h01A' ) > 0 )
    set gCond_AttackWithSasquatches = ( GetUnitCount( 'n012' ) > 0 )
    set gCond_AttackWithOgres = ( GetUnitCount( 'n048' ) > 0 )
    set gCond_AttackWithDragonHawks = ( GetUnitCount( 'h01D' ) > 0 )
    set gCond_AttackWithGryphons = ( GetUnitCount( 'h01C' ) > 0 )
    set gCond_MinimumBandits = ( GetUnitCountDone( 'h00B' ) > 0 )
    set gCond_AtacarAEnemigo = ( FoodUsed(  ) > 120 )
    set gCond_Farmeo_avanzado = ( FoodUsed(  ) > 90 )
    set gCond_Farmeo_Mediano = ( FoodUsed(  ) > 60 )
    set gCond_NeedExpansionRequiere = ( ( FoodUsed(  ) > 45 ) and ( ExpansionNeeded(  ) == true ) )
    set gCond_RushCommand = ( CheckLastCommand( false ) == 1 )
    set gCond_RushForces = ( FoodUsed(  ) < 50 )
endfunction

