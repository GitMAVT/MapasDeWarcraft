//===========================================================================
// Updates the values of all preset conditions
//===========================================================================
function UpdateConditions takes nothing returns nothing
    set gCond_BuildGeneral = ( TownThreatened(  ) == false )
    set gCond_BuildTowers = ( ( TownThreatened(  ) == false ) and ( ( ( GetUnitCount( 'dbgt' ) + GetUnitCount( 'dbmt' ) ) + ( GetUnitCount( 'dbbt' ) + GetUnitCount( 'dbft' ) ) ) < 12 ) )
    set gCond_BuildTownHall = ( ( TownThreatened(  ) == false ) and ( ( GetUnitCountDone( 'dbth' ) == 0 ) and ( ( GetUnitCount( 'dbkh' ) == 0 ) and ( GetUnitCount( 'dbch' ) == 0 ) ) ) )
    set gCond_BuildKeep = ( ( TownThreatened(  ) == false ) and ( ( GetUnitCountDone( 'dbth' ) > 0 ) and ( ( GetUnitCount( 'dbkh' ) == 0 ) and ( GetUnitCount( 'dbch' ) == 0 ) ) ) )
    set gCond_BuildCastle = ( ( TownThreatened(  ) == false ) and ( ( GetUnitCountDone( 'dbkh' ) > 0 ) and ( GetUnitCount( 'dbch' ) == 0 ) ) )
    set gCond_BuildFarms = ( ( TownThreatened(  ) == false ) and ( ( 15* GetUnitCount( 'dbfp' ) < 200 ) and ( ( 15 * GetUnitCount( 'dbfp' ) ) - FoodUsed(  )  ) < 10 ) )
    set gCond_Workers1 = ( ( GetUnitCount( 'dumi' ) < 35 ) and ( ( TownHasMine( ( TownWithMine(  ) + 0 ) ) == true ) and ( TownHasHall( ( TownWithMine(  ) + 0 ) ) == true ) ) )
    set gCond_Workers2 = ( ( GetUnitCount( 'dumi' ) < 35 ) and ( ( TownHasMine( ( TownWithMine(  ) + 1 ) ) == true ) and ( TownHasHall( ( TownWithMine(  ) + 1 ) ) == true ) ) )
    set gCond_Workers3 = ( ( GetUnitCount( 'dumi' ) < 35 ) and ( ( TownHasMine( ( TownWithMine(  ) + 3 ) ) == true ) and ( TownHasHall( ( TownWithMine(  ) + 3 ) ) == true ) ) )
    set gCond_NeedExp2 = ( GetMinesOwned(  ) < 2 )
    set gCond_NeedExp3 = ( GetMinesOwned(  ) < 3 )
    set gCond_HeroFirst = ( GetUnitCountDone( 'dbha' ) > 0 )
    set gCond_HeroSecond = ( ( GetUnitCountDone( 'dbha' ) > 0 ) and ( ( GetUnitCountDone( 'dbkh' ) > 0 ) or ( GetUnitCountDone( 'dbch' ) > 0 ) ) )
    set gCond_HeroThird = ( ( GetUnitCountDone( 'dbha' ) > 0 ) and ( GetUnitCountDone( 'dbch' ) > 0 ) )
    set gCond_AttackWithArchers = ( GetUnitCountDone( 'duag' ) > 0 )
    set gCond_AttackWithKnowledgeGuardians = ( GetUnitCountDone( 'dudg' ) > 0 )
    set gCond_AttackWithKirinTorMagiOgres = ( GetUnitCountDone( 'n00P' ) > 0 )
    set gCond_AttackWithGuardianGolems = ( GetUnitCountDone( 'duwg' ) > 0 )
    set gCond_AttackWithMagicPrismes = ( GetUnitCountDone( 'dump' ) > 0 )
    set gCond_AttackWithDalaranRevenant = ( GetUnitCountDone( 'n00D' ) > 0 )
    set gCond_AttackWithBattleHealer = ( GetUnitCountDone( 'duwm' ) > 0 )
    set gCond_AttackWithElementalMagicians = ( GetUnitCountDone( 'e008' ) > 0 )
    set gCond_AttackWithAirElementals = ( GetUnitCountDone( 'duae' ) > 0 )
    set gCond_AttackWithWaterDragons = ( GetUnitCountDone( 'duwd' ) > 0 )
    set gCond_AttackWithPhoenixes = ( GetUnitCountDone( 'duph' ) > 0 )
    set gCond_AtacarAEnemigo = ( FoodUsed(  ) > 100 )
    set gCond_Farmeo_avanzado = ( FoodUsed(  ) > 87 )
    set gCond_Farmeo_Mediano = ( FoodUsed(  ) > 57 )
