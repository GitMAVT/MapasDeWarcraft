//===========================================================================
// Updates the values of all preset conditions
//===========================================================================
function UpdateConditions takes nothing returns nothing
    set gCond_BuildGeneral = ( TownThreatened(  ) == false )
    set gCond_BuildFarms = ( ( TownThreatened(  ) == false ) and ( ( ( 15 * GetUnitCount( 'juik' ) ) < 200 ) and ( ( ( 15 * GetUnitCount( 'juik' ) ) - FoodUsed(  ) ) < 10 ) ) )
    set gCond_BuildTowers = ( ( TownThreatened(  ) == false ) and ( GetUnitCount( 'nbt2' ) < 10 ) )
    set gCond_BuildGreatHall = ( ( TownThreatened(  ) == false ) and ( ( GetUnitCount( 'hytg' ) == 0 ) and ( ( GetUnitCount( 'hdvg' ) == 0 ) and ( GetUnitCount( 'afds' ) == 0 ) ) ) )
    set gCond_BuildStronghold = ( ( TownThreatened(  ) == false ) and ( ( GetUnitCountDone( 'hytg' ) > 0 ) and ( ( GetUnitCount( 'hdvg' ) == 0 ) and ( GetUnitCount( 'afds' ) == 0 ) ) ) )
    set gCond_BuildFortress = ( ( TownThreatened(  ) == false ) and ( ( GetUnitCountDone( 'hdvg' ) > 0 ) and ( GetUnitCount( 'afds' ) == 0 ) ) )
    set gCond_NeedExp2 = ( GetMinesOwned(  ) < 2 )
    set gCond_NeedExp3 = ( GetMinesOwned(  ) < 3 )
    set gCond_Workers1 = ( ( GetUnitCount( 'd231' ) < 35 ) and ( ( TownHasMine( ( TownWithMine(  ) + 0 ) ) == true ) and ( TownHasHall( ( TownWithMine(  ) + 0 ) ) == true ) ) )
    set gCond_Workers2 = ( ( GetUnitCount( 'd231' ) < 35 ) and ( ( TownHasMine( ( TownWithMine(  ) + 1 ) ) == true ) and ( TownHasHall( ( TownWithMine(  ) + 1 ) ) == true ) ) )
    set gCond_Workers3 = ( ( GetUnitCount( 'd231' ) < 35 ) and ( ( TownHasMine( ( TownWithMine(  ) + 2 ) ) == true ) and ( TownHasHall( ( TownWithMine(  ) + 2 ) ) == true ) ) )
    set gCond_HeroFirst = ( GetUnitCountDone( 'n61H' ) > 0 )
    set gCond_HeroSecond = ( ( GetUnitCountDone( 'n61H' ) > 0 ) and ( ( GetUnitCountDone( 'afds' ) > 0 ) or ( GetUnitCountDone( 'hdvg' ) > 0 ) ) )
    set gCond_HeroThird = ( ( GetUnitCountDone( 'n61H' ) > 0 ) and ( GetUnitCountDone( 'afds' ) > 0 ) )
    set gCond_AttackWithVindicators = ( GetUnitCountDone( 'd010' ) > 0 )
    set gCond_AttackWithRockThrowers = ( GetUnitCountDone( 'n01D' ) > 0 )
    set gCond_AttackWithCatapult = ( GetUnitCountDone( 'o02J' ) > 0 )
    set gCond_AttackWithHarbingers = ( GetUnitCountDone( 'n01B' ) > 0 )
    set gCond_AttackWithWoodDruids = ( GetUnitCountDone( 'd006' ) > 0 )
    set gCond_AttackWithManaCarriers = ( GetUnitCountDone( 'n00X' ) > 0 )
    set gCond_AttackWithSalamanders = ( GetUnitCountDone( 'd111' ) > 0 )
    set gCond_AttackWithNetherRayes = ( GetUnitCountDone( 'n01E' ) > 0 )
    set gCond_AttackWithFREDs = ( GetUnitCountDone( 'd050' ) > 0 )
    set gCond_AttackWithWoodGuardian = ( GetUnitCountDone( 'n014' ) > 0 )
    set gCond_AttackWithFireDisciples = ( GetUnitCountDone( 'f012' ) > 0 )
    set gCond_AtacarAEnemigo = ( FoodUsed(  ) > 100 )
    set gCond_Farmeo_avanzado = ( FoodUsed(  ) > 87 )
    set gCond_Farmeo_Mediano = ( FoodUsed(  ) > 57 )
    set gCond_NeedExpansionRequiere = ( ( FoodUsed(  ) > 42 ) and ( ExpansionNeeded(  ) == true ) )
    set gCond_RushCommand = ( CheckLastCommand( false ) == 1 )
    set gCond_RushForces = ( FoodUsed(  ) < 75 )
endfunction

