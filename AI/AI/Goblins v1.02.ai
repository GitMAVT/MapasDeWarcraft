//===========================================================================
// Updates the values of all preset conditions
//===========================================================================
function UpdateConditions takes nothing returns nothing
    set gCond_BuildGeneral = ( TownThreatened(  ) == false )
    set gCond_BuildFarm = ( ( TownThreatened(  ) == false ) and ( ( ( GetUnitCount( 'o01A' ) * 20 ) < 200 ) and ( ( ( GetUnitCount( 'o01A' ) * 20 ) - FoodUsed(  ) ) < 20 ) ) )
    set gCond_BuildTowers = ( ( TownThreatened(  ) == false ) and ( ( GetUnitCount( 'o01F' ) + GetUnitCount( 'o01E' ) ) < 10 ) )
    set gCond_BuildTownHall = ( ( TownThreatened(  ) == false ) and ( ( GetUnitCount( 'o018' ) == 0 ) and ( ( GetUnitCount( 'o016' ) == 0 ) and ( GetUnitCount( 'o017' ) == 0 ) ) ) )
    set gCond_BuildKeep = ( ( TownThreatened(  ) == false ) and ( ( GetUnitCountDone( 'o018' ) > 0 ) and ( ( GetUnitCount( 'o016' ) == 0 ) and ( GetUnitCount( 'o017' ) == 0 ) ) ) )
    set gCond_BuildCastle = ( ( TownThreatened(  ) == false ) and ( ( GetUnitCountDone( 'o016' ) > 0 ) and ( GetUnitCount( 'o017' ) == 0 ) ) )
    set gCond_HeroFirst = ( GetUnitCountDone( 'o01G' ) > 0 )
    set gCond_HeroSecond = ( ( GetUnitCountDone( 'o01G' ) > 0 ) and ( ( GetUnitCountDone( 'o016' ) > 0 ) or ( GetUnitCountDone( 'o017' ) > 0 ) ) )
    set gCond_HeroThird = ( ( GetUnitCountDone( 'o01G' ) > 0 ) and ( GetUnitCountDone( 'o017' ) > 0 ) )
    set gCond_NeedExp = ( GetMinesOwned(  ) < 2 )
    set gCond_NeedExp2 = ( GetMinesOwned(  ) < 3 )
    set gCond_Workers1 = ( ( GetUnitCount( 'ug01' ) < 15 ) and ( ( TownHasMine( ( TownWithMine(  ) + 0 ) ) == true ) and ( TownHasHall( ( TownWithMine(  ) + 0 ) ) == true ) ) )
    set gCond_Workers2 = ( ( GetUnitCount( 'ug01' ) < 15 ) and ( ( TownHasMine( ( TownWithMine(  ) + 1 ) ) == true ) and ( TownHasHall( ( TownWithMine(  ) + 1 ) ) == true ) ) )
    set gCond_Workers3 = ( ( GetUnitCount( 'ug01' ) < 15 ) and ( ( TownHasMine( ( TownWithMine(  ) + 2 ) ) == true ) and ( TownHasHall( ( TownWithMine(  ) + 2 ) ) == true ) ) )
    set gCond_AttackWithGunner = ( GetUnitCountDone( 'ug02' ) > 0 )
    set gCond_AttackWithClockwerkWarriors = ( GetUnitCountDone( 'ug04' ) > 0 )
    set gCond_AttackWithEngineers = ( GetUnitCountDone( 'ug05' ) > 0 )
    set gCond_AttackWithTNTCatapults = ( GetUnitCountDone( 'ug07' ) > 0 )
    set gCond_AttackWithOgreSquads = ( GetUnitCountDone( 'ug11' ) > 0 )
    set gCond_AttackWithSappers = ( GetUnitCountDone( 'ngsp' ) > 0 )
    set gCond_AttackWithAntAirJunks = ( GetUnitCountDone( 'ug06' ) > 0 )
    set gCond_AttackWithFlyingContraptions = ( GetUnitCountDone( 'ug08' ) > 0 )
    set gCond_AttackWithTanks = ( GetUnitCountDone( 'ug09' ) > 0 )
    set gCond_Shreders1 = ( ( GetUnitCount( 'ug03' ) < 5 ) and ( ( TownHasMine( ( TownWithMine(  ) + 0 ) ) == true ) and ( TownHasHall( ( TownWithMine(  ) + 0 ) ) == true ) ) )
    set gCond_Shreders2 = ( ( GetUnitCount( 'ug03' ) < 5 ) and ( ( TownHasMine( ( TownWithMine(  ) + 1 ) ) == true ) and ( TownHasHall( ( TownWithMine(  ) + 1 ) ) == true ) ) )
    set gCond_Shreders3 = ( ( GetUnitCount( 'ug03' ) < 5 ) and ( ( TownHasMine( ( TownWithMine(  ) + 2 ) ) == true ) and ( TownHasHall( ( TownWithMine(  ) + 2 ) ) == true ) ) )
    set gCond_AtacarAEnemigo = ( FoodUsed(  ) > 100 )
    set gCond_Farmeo_avanzado = ( FoodUsed(  ) > 87 )
    set gCond_Farmeo_Mediano = ( FoodUsed(  ) > 57 )
    set gCond_NeedExpansionRequiere = ( ( FoodUsed(  ) > 42 ) and ( ExpansionNeeded(  ) == true ) )
    set gCond_RushCommand = ( CheckLastCommand( false ) == 1 )
    set gCond_RushForces = ( FoodUsed(  ) < 75 )
endfunction

