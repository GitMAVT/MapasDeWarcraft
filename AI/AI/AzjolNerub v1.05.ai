//===========================================================================
// Updates the values of all preset conditions
//===========================================================================
function UpdateConditions takes nothing returns nothing
    set gCond_BuildGeneral = ( TownThreatened(  ) == false )
    set gCond_BuildZiggurat = ( ( TownThreatened(  ) == false ) and ( ( ( ( GetUnitCount( 'n052' ) + GetUnitCount( 'n000' ) + GetUnitCount( 'n00W' ) ) * 20 ) < 200 ) and ( ( ( ( GetUnitCount( 'n052' ) + GetUnitCount( 'n000' ) + GetUnitCount( 'n00W' ) ) * 20 ) - FoodUsed(  ) ) < 20 ) ) )
    set gCond_BuildNecropolis = ( ( TownThreatened(  ) == false ) and ( ( GetUnitCount( 'n009' ) == 0 ) and ( ( GetUnitCount( 'n601' ) == 0 ) and ( GetUnitCount( 'n00R' ) == 0 ) ) ) )
    set gCond_BuildHallsOfWeb = ( ( TownThreatened(  ) == false ) and ( ( GetUnitCountDone( 'n009' ) > 0 ) and ( ( GetUnitCount( 'n601' ) == 0 ) and ( GetUnitCount( 'n00R' ) == 0 ) ) ) )
    set gCond_BuildSpiderCitadel = ( ( TownThreatened(  ) == false ) and ( ( GetUnitCountDone( 'n601' ) > 0 ) and ( GetUnitCount( 'n00R' ) == 0 ) ) )
    set gCond_BuildTowers = ( ( GetUnitCountDone( 'n052' ) > 0 ) and ( ( GetUnitCount( 'n000' ) + GetUnitCount( 'n00W' ) ) < 5 ) )
    set gCond_NeedExp = ( GetMinesOwned(  ) < 2 )
    set gCond_NeedExp2 = ( GetMinesOwned(  ) < 3 )
    set gCond_HeroFirst = ( GetUnitCountDone( 'n602' ) > 0 )
    set gCond_HeroSecond = ( ( GetUnitCountDone( 'n602' ) > 0 ) and ( ( GetUnitCountDone( 'n601' ) > 0 ) or ( GetUnitCountDone( 'n00R' ) > 0 ) ) )
    set gCond_HeroThird = ( ( GetUnitCountDone( 'n602' ) > 0 ) and ( GetUnitCountDone( 'n00R' ) == 0 ) )
    set gCond_Workers1 = ( ( GetUnitCount( 'n600' ) < 35 ) and ( ( TownHasMine( ( TownWithMine(  ) + 0 ) ) == true ) and ( TownHasHall( ( TownWithMine(  ) + 0 ) ) == true ) ) )
    set gCond_Workers2 = ( ( GetUnitCount( 'n600' ) < 35 ) and ( ( TownHasMine( ( TownWithMine(  ) + 1 ) ) == true ) and ( TownHasHall( ( TownWithMine(  ) + 1 ) ) == true ) ) )
    set gCond_Workers3 = ( ( GetUnitCount( 'n600' ) < 35 ) and ( ( TownHasMine( ( TownWithMine(  ) + 2 ) ) == true ) and ( TownHasHall( ( TownWithMine(  ) + 2 ) ) == true ) ) )
    set gCond_AttackWithGeists = ( GetUnitCountDone( 'n00U' ) > 0 )
    set gCond_AttackWithCryptFiends = ( GetUnitCountDone( 'u600' ) > 0 )
    set gCond_AttackWithCryptFlyers = ( GetUnitCountDone( 'u000' ) > 0 )
    set gCond_AttackWithCryptWraith = ( GetUnitCountDone( 'n00V' ) > 0 )
    set gCond_AttackWithCryptSpider = ( GetUnitCountDone( 'n00T' ) > 0 )
    set gCond_AttackWithCryptMothers = ( GetUnitCountDone( 'n003' ) > 0 )
    set gCond_AttackWithCryptArachnathids = ( GetUnitCountDone( 'n629' ) > 0 )
    set gCond_AttackWithCryptInterceptor = ( GetUnitCountDone( 'nub7' ) > 0 )
    set gCond_AttackWithCryptGuardian = ( GetUnitCountDone( 'u005' ) > 0 )
    set gCond_AttackWithFlameWyrm = ( GetUnitCountDone( 'n00S' ) > 0 )
    set gCond_AttackWithShade = ( GetUnitCountDone( 'ushd' ) > 0 )
    set gCond_AtacarAEnemigo = ( FoodUsed(  ) > 100 )
    set gCond_Farmeo_avanzado = ( FoodUsed(  ) > 87 )
    set gCond_Farmeo_Mediano = ( FoodUsed(  ) > 57 )
    set gCond_NeedExpansionRequiere = ( ( FoodUsed(  ) > 42 ) and ( ExpansionNeeded(  ) == true ) )
    set gCond_RushCommand = ( CheckLastCommand( false ) == 1 )
