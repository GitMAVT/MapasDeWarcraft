    set gCond_Workers2 = ( ( GetUnitCount( 'ku01' ) < 45 ) and ( ( TownHasMine( ( TownWithMine(  ) + 1 ) ) == true ) and ( TownHasHall( ( TownWithMine(  ) + 1 ) ) == true ) ) )
    set gCond_Workers3 = ( ( GetUnitCount( 'ku01' ) < 45 ) and ( ( TownHasMine( ( TownWithMine(  ) + 2 ) ) == true ) and ( TownHasHall( ( TownWithMine(  ) + 2 ) ) == true ) ) )
    set gCond_NeedExp2 = ( GetMinesOwned(  ) < 2 )
    set gCond_NeedExp3 = ( GetMinesOwned(  ) < 3 )
    set gCond_AttackWithFootmen = ( GetUnitCount( 'ku02' ) > 0 )
