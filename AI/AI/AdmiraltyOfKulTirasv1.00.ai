    set gCond_AttackWithSteamTanks = ( GetUnitCount( 'h014' ) > 0 )
    set gCond_AttackWithDruidOfTheTalon = ( GetUnitCount( 'n03U' ) > 0 )
    set gCond_AttackWithGryphonRider = ( GetUnitCount( 'h028' ) > 0 )
    set gCond_MinimumDruids = ( ( GetUnitCount( 'kb10' ) > 0 ) and ( GetUpgradeLevel( 'R03U' ) > 0 ) )
