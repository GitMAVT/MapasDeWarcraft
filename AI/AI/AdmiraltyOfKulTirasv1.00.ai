    set gCond_HeroThird = ( ( GetUnitCountDone( 'kb05' ) > 0 ) and ( GetUnitCountDone( 'kb03' ) > 0 ) )
    set gCond_BuildFarms = ( ( TownThreatened(  ) == false ) and ( ( TotalFoodProduced(  ) < 200 ) and ( ( TotalFoodProduced(  ) - FoodUsed(  ) ) < 15 ) ) )
