//===========================================================================
// Updates the values of all preset conditions
//===========================================================================
function UpdateConditions takes nothing returns nothing
    set gCond_ConstructBuildingGeneral = ( TownThreatened(  ) == false )
    set gCond_BuildBurrows = ( ( TownThreatened(  ) == false ) and ( ( TotalFoodProduced(  ) < 200 ) and ( ( TotalFoodProduced(  ) - FoodUsed(  ) ) < 10 ) ) )
    set gCond_HeroFirst = ( GetUnitCountDone( 'oalt' ) > 0 )
    set gCond_HeroSecond = ( ( GetUnitCountDone( 'oalt' ) > 0 ) and ( ( GetUnitCountDone( 'ostr' ) > 0 ) or ( GetUnitCountDone( 'ofrt' ) > 0 ) ) )
    set gCond_HeroThird = ( ( GetUnitCountDone( 'oalt' ) > 0 ) and ( GetUnitCountDone( 'ofrt' ) > 0 ) )
    set gCond_TrainGrunt = ( ( GetUnitCountDone( 'ofrt' ) == 0 ) or ( GetUnitCountDone( 'otto' ) == 0 ) )
    set gCond_TrainHeadHunter = ( GetUpgradeLevel( 'Robk' ) == 0 )
    set gCond_TrainBerserkers = ( GetUpgradeLevel( 'Robk' ) != 0 )
    set gCond_BuildGreatHall = ( ( TownThreatened(  ) == false ) and ( ( GetUnitCount( 'ogre' ) == 0 ) and ( ( GetUnitCount( 'ostr' ) == 0 ) and ( GetUnitCount( 'ofrt' ) == 0 ) ) ) )
    set gCond_UpgradeStronghold = ( ( TownThreatened(  ) == false ) and ( ( GetUnitCount( 'ogre' ) > 0 ) and ( ( GetUnitCount( 'ostr' ) == 0 ) and ( GetUnitCount( 'ofrt' ) == 0 ) ) ) )
    set gCond_UpgradeFortress = ( ( TownThreatened(  ) == false ) and ( ( GetUnitCount( 'ostr' ) > 0 ) and ( GetUnitCount( 'ofrt' ) == 0 ) ) )
    set gCond_WorkersMine1 = ( ( GetUnitCount( 'opeo' ) < 35 ) and ( ( TownHasMine( ( TownWithMine(  ) + 0 ) ) == true ) and ( TownHasHall( ( TownWithMine(  ) + 0 ) ) == true ) ) )
    set gCond_WorkersMine2 = ( ( GetUnitCount( 'opeo' ) < 35 ) and ( ( TownHasMine( ( TownWithMine(  ) + 1 ) ) == true ) and ( TownHasHall( ( TownWithMine(  ) + 1 ) ) == true ) ) )
    set gCond_BuildWatchTowers = ( ( TownThreatened(  ) == false ) and ( GetUnitCountDone( 'owtw' ) < 5 ) )
    set gCond_NeedExp = ( GetMinesOwned(  ) < 2 )
    set gCond_AttackWithTaurensMinium = ( ( GetUnitCountDone( 'ofrt' ) > 0 ) and ( GetUnitCountDone( 'otto' ) > 0 ) )
    set gCond_AttackWithGrunts = ( GetUnitCount( 'ogru' ) > 0 )
    set gCond_AttackWithHeadHunters = ( GetUnitCount( 'ohun' ) > 0 )
    set gCond_AttackWithBerserkers = ( GetUnitCount( 'otbk' ) > 0 )
    set gCond_AttackWithCatapults = ( GetUnitCount( 'ocat' ) > 0 )
    set gCond_AttackWithShamans = ( GetUnitCount( 'oshm' ) > 0 )
    set gCond_AttackWithWitchDoctors = ( GetUnitCount( 'odoc' ) > 0 )
    set gCond_AttackWithRaiders = ( GetUnitCount( 'orai' ) > 0 )
    set gCond_AttackWithWindRider = ( GetUnitCount( 'owyv' ) > 0 )
    set gCond_AttackWithKodos = ( GetUnitCount( 'okod' ) > 0 )
    set gCond_AttackWithBatriders = ( GetUnitCount( 'otbr' ) > 0 )
    set gCond_AttackWithSpiritWalker = ( GetUnitCount( 'ospm' ) > 0 )
    set gCond_AttackWithTaurens = ( GetUnitCount( 'otau' ) > 0 )
    set gCond_WorkersMine3 = ( ( GetUnitCount( 'opeo' ) < 35 ) and ( ( TownHasMine( ( TownWithMine(  ) + 2 ) ) == true ) and ( TownHasHall( ( TownWithMine(  ) + 2 ) ) == true ) ) )
    set gCond_NeedExp2 = ( GetMinesOwned(  ) < 3 )
    set gCond_AtacarAEnemigo = ( FoodUsed(  ) > 100 )
    set gCond_Farmeo_avanzado = ( FoodUsed(  ) > 87 )
    set gCond_Farmeo_Mediano = ( FoodUsed(  ) > 57 )
    set gCond_NeedExpansionRequiere = ( ( FoodUsed(  ) > 42 ) and ( ExpansionNeeded(  ) == true ) )
    set gCond_RushCommand = ( CheckLastCommand( false ) == 1 )
    set gCond_RushForces = ( FoodUsed(  ) < 75 )
endfunction

