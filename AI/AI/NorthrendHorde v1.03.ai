//===========================================================================
// Updates the values of all preset conditions
//===========================================================================
function UpdateConditions takes nothing returns nothing
    set gCond_BuildGeneral = ( TownThreatened(  ) == false )
    set gCond_BuildFarm = ( ( TownThreatened(  ) == false ) and ( ( ( 20 * GetUnitCount( 'o00V' ) ) < 200 ) and ( ( ( 20 * GetUnitCount( 'o00V' ) ) - FoodUsed(  ) ) < 10 ) ) )
    set gCond_BuildTower = ( ( TownThreatened(  ) == false ) and ( GetUnitCount( 'o010' ) < 5 ) )
    set gCond_BuildGreatHall = ( ( TownThreatened(  ) == false ) and ( ( GetUnitCountDone( 'o00Y' ) == 0 ) and ( ( GetUnitCount( 'o00R' ) == 0 ) and ( GetUnitCount( 'o00U' ) == 0 ) ) ) )
    set gCond_BuildStronghold = ( ( TownThreatened(  ) == false ) and ( ( GetUnitCountDone( 'o00Y' ) > 0 ) and ( ( GetUnitCount( 'o00R' ) == 0 ) and ( GetUnitCount( 'o00U' ) == 0 ) ) ) )
    set gCond_BuildFortress = ( ( TownThreatened(  ) == false ) and ( ( GetUnitCountDone( 'o00R' ) > 0 ) and ( GetUnitCount( 'o00U' ) == 0 ) ) )
    set gCond_HeroFirst = ( GetUnitCountDone( 'o00Q' ) > 0 )
    set gCond_HeroSecond = ( ( GetUnitCountDone( 'o00Q' ) > 0 ) and ( ( GetUnitCountDone( 'o00R' ) > 0 ) or ( GetUnitCountDone( 'o00U' ) > 0 ) ) )
    set gCond_HeroThird = ( ( GetUnitCountDone( 'o00Q' ) > 0 ) and ( GetUnitCountDone( 'o00U' ) > 0 ) )
    set gCond_NeedExp2 = ( GetMinesOwned(  ) < 2 )
    set gCond_NeedExp3 = ( GetMinesOwned(  ) < 3 )
    set gCond_Workers1 = ( ( GetUnitCount( 'o012' ) < 34 ) and ( ( TownHasMine( ( TownWithMine(  ) + 0 ) ) == true ) and ( TownHasHall( ( TownWithMine(  ) + 0 ) ) == true ) ) )
    set gCond_Workers2 = ( ( GetUnitCount( 'o012' ) < 34 ) and ( ( TownHasMine( ( TownWithMine(  ) + 1 ) ) == true ) and ( TownHasHall( ( TownWithMine(  ) + 1 ) ) == true ) ) )
    set gCond_Workers3 = ( ( GetUnitCount( 'o012' ) < 34 ) and ( ( TownHasMine( ( TownWithMine(  ) + 2 ) ) == true ) and ( TownHasHall( ( TownWithMine(  ) + 2 ) ) == true ) ) )
    set gCond_TrainGrunts = ( ( GetUnitCountDone( 'o00U' ) == 0 ) and ( GetUnitCountDone( 'o011' ) == 0 ) )
    set gCond_AttackWithGrunts = ( GetUnitCountDone( 'n020' ) > 0 )
    set gCond_AttackWithTuskarrHunters = ( GetUnitCountDone( 'n021' ) > 0 )
    set gCond_AttackWithDemolishers = ( GetUnitCountDone( 'n029' ) > 0 )
    set gCond_AttackWithShamans = ( GetUnitCountDone( 'n023' ) > 0 )
    set gCond_AttackWithHealers = ( GetUnitCountDone( 'n022' ) > 0 )
    set gCond_AttackWithRaiders = ( GetUnitCountDone( 'n025' ) > 0 )
    set gCond_AttackWithEliteBears = ( GetUnitCountDone( 'n027' ) > 0 )
    set gCond_AttackWithMammoths = ( GetUnitCountDone( 'n026' ) > 0 )
    set gCond_AttackWithFrostDrakes = ( GetUnitCountDone( 'n02A' ) > 0 )
    set gCond_AttackWithWalkers = ( GetUnitCountDone( 'n028' ) > 0 )
    set gCond_AttackWithTaunkas = ( GetUnitCountDone( 'n024' ) > 0 )
    set gCond_Minimun_Taukas = ( ( GetUnitCountDone( 'o00U' ) > 0 ) and ( GetUnitCountDone( 'o011' ) > 0 ) )
    set gCond_AtacarAEnemigo = ( FoodUsed(  ) > 100 )
    set gCond_Farmeo_avanzado = ( FoodUsed(  ) > 87 )
    set gCond_Farmeo_Mediano = ( FoodUsed(  ) > 57 )
