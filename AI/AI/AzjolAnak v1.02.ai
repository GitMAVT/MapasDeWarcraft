//===========================================================================
// Updates the values of all preset conditions
//===========================================================================
function UpdateConditions takes nothing returns nothing
    set gCond_BuildGeneral = ( TownThreatened(  ) == false )
    set gCond_BuildZiggurat = ( ( TownThreatened(  ) == false ) and ( ( ( ( GetUnitCount( 'nab0' ) + GetUnitCount( 'naa1' ) + GetUnitCount( 'naa2' ) ) * 20 ) < 200 ) and ( ( ( ( GetUnitCount( 'nab0' ) + GetUnitCount( 'naa1' ) + GetUnitCount( 'naa2' ) ) * 20 ) - FoodUsed(  ) ) < 20 ) ) )
    set gCond_BuildTowers = ( ( TownThreatened(  ) == false ) and ( ( GetUnitCount( 'naa1' ) + GetUnitCount( 'naa2' ) ) < 10 ) )
    set gCond_BuildTownHall = ( ( TownThreatened(  ) == false ) and ( ( GetUnitCount( 'nab7' ) == 0 ) and ( ( GetUnitCount( 'nab6' ) == 0 ) and ( GetUnitCount( 'nab5' ) == 0 ) ) ) )
    set gCond_BuildStronghold = ( ( TownThreatened(  ) == false ) and ( ( GetUnitCountDone( 'nab7' ) > 0 ) and ( ( GetUnitCount( 'nab6' ) == 0 ) and ( GetUnitCount( 'nab5' ) == 0 ) ) ) )
    set gCond_BuildPalace = ( ( TownThreatened(  ) == false ) and ( ( GetUnitCountDone( 'nab6' ) > 0 ) and ( GetUnitCount( 'nab5' ) == 0 ) ) )
    set gCond_NeedExp = ( GetMinesOwned(  ) < 2 )
    set gCond_NeedExp2 = ( GetMinesOwned(  ) < 3 )
    set gCond_Workers1 = ( ( GetUnitCount( 'na01' ) < 35 ) and ( ( TownHasMine( ( TownWithMine(  ) + 0 ) ) == true ) and ( TownHasHall( ( TownWithMine(  ) + 0 ) ) == true ) ) )
    set gCond_Workers2 = ( ( GetUnitCount( 'na01' ) < 35 ) and ( ( TownHasMine( ( TownWithMine(  ) + 1 ) ) == true ) and ( TownHasHall( ( TownWithMine(  ) + 1 ) ) == true ) ) )
    set gCond_Workers3 = ( ( GetUnitCount( 'na01' ) < 35 ) and ( ( TownHasMine( ( TownWithMine(  ) + 2 ) ) == true ) and ( TownHasHall( ( TownWithMine(  ) + 2 ) ) == true ) ) )
    set gCond_HeroFirst = ( GetUnitCountDone( 'nab1' ) > 0 )
    set gCond_HeroSecond = ( ( GetUnitCountDone( 'nab1' ) > 0 ) and ( ( GetUnitCountDone( 'nab6' ) > 0 ) or ( GetUnitCountDone( 'nab5' ) > 0 ) ) )
    set gCond_HeroThird = ( ( GetUnitCountDone( 'nab1' ) > 0 ) and ( GetUnitCountDone( 'nab5' ) > 0 ) )
    set gCond_AttackWithWarriors = ( GetUnitCountDone( 'nau2' ) > 0 )
    set gCond_AttackWithArchers = ( GetUnitCountDone( 'nau4' ) > 0 )
    set gCond_AttackWithMarauders = ( GetUnitCountDone( 'nau6' ) > 0 )
    set gCond_AttackWithAlphas = ( GetUnitCountDone( 'nau0' ) > 0 )
    set gCond_AttackWithPoisonSpiders = ( GetUnitCountDone( 'nau1' ) > 0 )
    set gCond_AttackWithInfectors = ( GetUnitCountDone( 'nau5' ) > 0 )
    set gCond_AttackWithBroodMothers = ( GetUnitCountDone( 'n02L' ) > 0 )
    set gCond_AttackWithFlyers = ( GetUnitCountDone( 'nau7' ) > 0 )
    set gCond_AttackWithWatchers = ( GetUnitCountDone( 'nau8' ) > 0 )
    set gCond_AttackWithBombardier = ( GetUnitCountDone( 'n053' ) > 0 )
    set gCond_AttackWithCryomancers = ( GetUnitCountDone( 'n03R' ) > 0 )
    set gCond_AttackWithWarlords = ( GetUnitCountDone( 'nau9' ) > 0 )
    set gCond_AtacarAEnemigo = ( FoodUsed(  ) > 100 )
    set gCond_Farmeo_avanzado = ( FoodUsed(  ) > 87 )
    set gCond_Farmeo_Mediano = ( FoodUsed(  ) > 57 )
    set gCond_NeedExpansionRequiere = ( ( FoodUsed(  ) > 42 ) and ( ExpansionNeeded(  ) == true ) )
