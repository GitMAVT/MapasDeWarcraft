//===========================================================================
// Updates the values of all preset conditions
//===========================================================================
function UpdateConditions takes nothing returns nothing
    set gCond_BuildGeneral = ( TownThreatened(  ) == false )
    set gCond_BuildZiggurats = ( ( TownThreatened(  ) == false ) and ( ( ( 20 * GetUnitCount( 'u00O' ) ) < 200 ) and ( ( ( 20 * GetUnitCount( 'u00O' ) ) - FoodUsed(  ) ) < 10 ) ) )
    set gCond_BuildNecropolis = ( ( TownThreatened(  ) == false ) and ( ( GetUnitCount( 'u00G' ) == 0 ) and ( ( GetUnitCount( 'u00L' ) == 0 ) and ( GetUnitCount( 'u00N' ) == 0 ) ) ) )
    set gCond_BuildHallsOfDeath = ( ( TownThreatened(  ) == false ) and ( ( GetUnitCountDone( 'u00G' ) > 0 ) and ( ( GetUnitCount( 'u00L' ) == 0 ) and ( GetUnitCount( 'u00N' ) == 0 ) ) ) )
    set gCond_BuildBlackCitadel = ( ( TownThreatened(  ) == false ) and ( ( GetUnitCountDone( 'u00L' ) > 0 ) and ( GetUnitCount( 'u00N' ) == 0 ) ) )
    set gCond_BuildNecroExp = ( ( TownThreatened(  ) == false ) and ( ( TownHasMine( ( TownWithMine(  ) + 1 ) ) == true ) and ( TownHasHall( ( TownWithMine(  ) + 1 ) ) == false ) ) )
    set gCond_BuildNecroExp2 = ( ( TownThreatened(  ) == false ) and ( ( TownHasMine( ( TownWithMine(  ) + 2 ) ) == true ) and ( TownHasHall( ( TownWithMine(  ) + 2 ) ) == false ) ) )
    set gCond_NeedExp = ( GetMinesOwned(  ) < 2 )
    set gCond_NeedExp2 = ( GetMinesOwned(  ) < 3 )
    set gCond_Workers1 = ( ( GetUnitCount( 'u000' ) < 15 ) and ( ( TownHasMine( ( TownWithMine(  ) + 0 ) ) == true ) and ( TownHasHall( ( TownWithMine(  ) + 0 ) ) == true ) ) )
    set gCond_Workers2 = ( ( GetUnitCount( 'u000' ) < 15 ) and ( ( TownHasMine( ( TownWithMine(  ) + 1 ) ) == true ) and ( TownHasHall( ( TownWithMine(  ) + 1 ) ) == true ) ) )
    set gCond_Workers3 = ( ( GetUnitCount( 'u000' ) < 15 ) and ( ( TownHasMine( ( TownWithMine(  ) + 2 ) ) == true ) and ( TownHasHall( ( TownWithMine(  ) + 2 ) ) == true ) ) )
    set gCond_HeroFirst = ( GetUnitCountDone( 'u00Q' ) > 0 )
    set gCond_HeroSecond = ( ( GetUnitCountDone( 'u00Q' ) > 0 ) and ( ( GetUnitCountDone( 'u00L' ) > 0 ) or ( GetUnitCountDone( 'u00N' ) > 0 ) ) )
    set gCond_HeroThird = ( ( GetUnitCountDone( 'u00Q' ) > 0 ) and ( GetUnitCountDone( 'u00N' ) > 0 ) )
    set gCond_AttackWithImps = ( GetUnitCountDone( 'o001' ) > 0 )
    set gCond_AttackWithFelGuards = ( GetUnitCountDone( 'n01K' ) > 0 )
    set gCond_AttackWithFireMephits = ( GetUnitCountDone( 'u007' ) > 0 )
    set gCond_AttackWithInfernalWagons = ( GetUnitCountDone( 'n00Q' ) > 0 )
    set gCond_AttackWithPitGuards = ( GetUnitCountDone( 'n01M' ) > 0 )
    set gCond_AttackWithCoreFelhounds = ( GetUnitCountDone( 'o002' ) > 0 )
    set gCond_AttackWithSucubuss = ( GetUnitCountDone( 'o008' ) > 0 )
    set gCond_AttackWithTaskmasters = ( GetUnitCountDone( 'n01J' ) > 0 )
    set gCond_AttackWithFelSerpents = ( GetUnitCountDone( 'n04S' ) > 0 )
    set gCond_AttackWithNetherDragons = ( GetUnitCountDone( 'n01L' ) > 0 )
    set gCond_AttackWithShades = ( GetUnitCountDone( 'ushd' ) > 0 )
    set gCond_AtacarAEnemigo = ( FoodUsed(  ) > 100 )
    set gCond_Farmeo_avanzado = ( FoodUsed(  ) > 87 )
    set gCond_Farmeo_Mediano = ( FoodUsed(  ) > 57 )
    set gCond_NeedExpansionRequiere = ( ( FoodUsed(  ) > 42 ) and ( ExpansionNeeded(  ) == true ) )
