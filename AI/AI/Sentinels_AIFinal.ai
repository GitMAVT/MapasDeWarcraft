    endif
    call SetBuildAll( BUILD_UNIT, 1, 'eate', -1 )
    call SetBuildAll( BUILD_UNIT, 1, 'eaom', -1 )
    call SetBuildAll( BUILD_UNIT, 1, 'emow', -1 )
    if (gCond_Workers1) then
        call SetBuildAll( BUILD_UNIT, 7, 'ewsp', -1 )
